@{
    ViewBag.Title = "About";
    string a = ViewBag.Message;
}

<div class="col-md-12">

    <div class="box" id="text-page" style="min-height: 448px;">
        <h1>About Us</h1>
        <div>
            @* Vince: http://stackoverflow.com/questions/25430648/displaying-html-string-from-model-in-mvc-razor-view *@
            @(new HtmlString(a))
        </div>
    </div>

</div>
<!-- /.col-md-9 -->
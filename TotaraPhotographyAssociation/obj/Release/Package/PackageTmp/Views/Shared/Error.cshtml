@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<div class="col-md-12">
    <div class="row" id="error-page">
        <div class="col-sm-6 col-sm-offset-3">
            <div class="box">

                <h3>Oops - something goes wrong</h3>

                <p class="buttons">
                    <a href="@Url.Action("Index", "Home", null, null)" class="btn btn-primary"><i class="fa fa-home"></i> Go to Homepage</a>
                </p>
            </div>
        </div>
    </div>
</div>